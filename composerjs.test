{
    "name": "bytefury/crater",
    "description": "Free & Open Source Invoice App for Freelancers & Small Businesses. https://craterapp.com",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-http": "*",
        "ext-json": "*",
        "authorizenet/authorizenet": "^2.0",
        "aws/aws-sdk-php": "^3.142",
        "barryvdh/laravel-dompdf": "^1.0",
        "desolatormagno/authorize-php": "^1.0",
        "desolatormagno/laravel-msg": "dev-master",
        "doctrine/dbal": "^2.13",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.3",
        "laravel/framework": "^9.52",
        "laravel/helpers": "^1.5",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/omnipay": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mavinoo/laravel-batch": "^2.3",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.2",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^5.0",
        "stripe/stripe-php": "^15.2",
        "vinkla/hashids": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-dump-server": "^1.8",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "jasonmccreary/laravel-test-assertions": "^2.1",
        "knuckleswtf/scribe": "^3.37",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.4"
    },
    "repositories": [],
    "autoload": {
        "psr-4": {
            "Crater\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Space/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.30"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
